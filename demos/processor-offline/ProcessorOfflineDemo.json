{
    "type": "OfflineGraphRenderer",
    "config": {
        "inputFiles": [
            {
                "filePath": "input.wav"
            }
        ],
        "outputFiles": [
            {
                "filePath": "output.wav",
                "numberOfChannels": 1
            }
        ],
        "graph": {
            "nodes": [
                {
                    "id": "processorNode",
                    "type": "ExampleDSP.ExampleProcessor",
                    "config": {
                        "gain": 0.2
                    }
                }
            ],
            "connections": [
                {
                    "sourceNode": "inputNode",
                    "destinationNode": "processorNode"
                },
                {
                    "sourceNode": "processorNode",
                    "destinationNode": "outputNode"
                }
            ]
        }
    }
}
